    </dir> <!-- /public_html/error -->
    <dir name="manual">
     <file name="index.php" role="www" />
    </dir> <!-- /public_html/manual -->
    <dir name="notes">
     <dir name="admin">
      <file name="index.php" role="www" />
      <file name="trans.php" role="www" />
      <file name="view-note.php" role="www" />
     </dir> <!-- /public_html/notes/admin -->
    </dir> <!-- /public_html/notes -->
   </dir> <!-- /public_html -->
